{
    "name": "microphork/application",
    "description": "The installer used to start a new microphork application",
    "keywords": ["microphork", "phork", "framework"],
    "require": {
        "microphork/framework": "1.0.*",
        "microphork/package-auth": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "Phork\\App\\": "app/classes"
        }
    },
    "scripts": {
        "post-create-project-cmd": "php composer/post-create-project.php"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}